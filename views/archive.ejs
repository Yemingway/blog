<%- include header %>
<ul class= "archive">
<% var lastYear = 0 %>
<% posts.forEach(function (post, index) { %>
  <% if(post.time.year != lastYear) { %>
  <li><h3><%= post.time.year%></h3></li>
  <% lastYear = post.time.year%>
  <% } %>
  <li><a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title%> </a><time><%= post.time.day%></time></li>
<% }) %>
</ul>
<%- include footer %>
